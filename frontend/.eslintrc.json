{
  "root": true,
  "extends": ["react-app", "plugin:prettier/recommended", "plugin:import/recommended"],
  "settings": {
    "react": { "version": "detect" }
  },
  "env": { "es6": true, "browser": true, "node": true },
  "rules": {
    "prettier/prettier": ["error", {}, { "usePrettierrc": true }],
    "import/order": ["error"]
  }
}
